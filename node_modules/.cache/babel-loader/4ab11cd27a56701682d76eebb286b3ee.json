{\n        text: 'Pending',\n        value: 'pending'\n      }, {\n        text: 'Canceled',\n        value: 'canceled'\n      }, {\n        text: 'Delivered',\n        value: 'delivered'\n      }, {\n        text: 'On Hold',\n        value: 'on_hold'\n      }],\n      settings: {\n        // perfectscrollbar settings\n        maxScrollbarLength: 60,\n        wheelSpeed: .60\n      }\n    };\n  },\n  computed: {\n    isSidebarActiveLocal: {\n      get: function get() {\n        return this.isSidebarActive;\n      },\n      set: function set(val) {\n        if (!val) {\n          this.$emit('closeSidebar');\n          this.initValues();\n        }\n      }\n    }\n  },\n  methods: {\n    initValues: function initValues() {\n      this.name = '';\n      this.category = 'audio';\n      this.order_status = 'pending';\n      this.price = '';\n      this.$refs.fileUpload.srcs = [];\n    }\n  },\n  components: {\n    VuePerfectScrollbar: VuePerfectScrollbar\n  }\n};",{"version":3,"sources":["AddNewDataSidebar.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkDA,OAAA,mBAAA,MAAA,uBAAA;AAEA,eAAA;AACA,EAAA,KAAA,EAAA;AACA,IAAA,eAAA,EAAA;AACA,MAAA,IAAA,EAAA,OADA;AAEA,MAAA,QAAA,EAAA;AAFA;AADA,GADA;AAOA,EAAA,IAPA,kBAOA;AACA,WAAA;AACA,MAAA,IAAA,EAAA,EADA;AAEA,MAAA,QAAA,EAAA,OAFA;AAGA,MAAA,YAAA,EAAA,SAHA;AAIA,MAAA,KAAA,EAAA,EAJA;AAMA,MAAA,gBAAA,EAAA,CACA;AAAA,QAAA,IAAA,EAAA,OAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OADA,EAEA;AAAA,QAAA,IAAA,EAAA,WAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAFA,EAGA;AAAA,QAAA,IAAA,EAAA,SAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAHA,EAIA;AAAA,QAAA,IAAA,EAAA,WAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAJA,CANA;AAYA,MAAA,oBAAA,EAAA,CACA;AAAA,QAAA,IAAA,EAAA,SAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OADA,EAEA;AAAA,QAAA,IAAA,EAAA,UAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAFA,EAGA;AAAA,QAAA,IAAA,EAAA,WAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAHA,EAIA;AAAA,QAAA,IAAA,EAAA,SAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAJA,CAZA;AAkBA,MAAA,QAAA,EAAA;AAAA;AACA,QAAA,kBAAA,EAAA,EADA;AAEA,QAAA,UAAA,EAAA;AAFA;AAlBA,KAAA;AAuBA,GA/BA;AAgCA,EAAA,QAAA,EAAA;AACA,IAAA,oBAAA,EAAA;AACA,MAAA,GADA,iBACA;AACA,eAAA,KAAA,eAAA;AACA,OAHA;AAIA,MAAA,GAJA,eAIA,GAJA,EAIA;AACA,YAAA,CAAA,GAAA,EAAA;AACA,eAAA,KAAA,CAAA,cAAA;AACA,eAAA,UAAA;AACA;AACA;AATA;AADA,GAhCA;AA6CA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,OAAA;AACA,WAAA,YAAA,GAAA,SAAA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,KAAA,CAAA,UAAA,CAAA,IAAA,GAAA,EAAA;AACA;AAPA,GA7CA;AAsDA,EAAA,UAAA,EAAA;AACA,IAAA,mBAAA,EAAA;AADA;AAtDA,CAAA","sourcesContent":["<!-- =========================================================================================\r\n  File Name: AddNewDataSidebar.vue\r\n  Description: Add New Data - Sidebar component\r\n  ----------------------------------------------------------------------------------------\r\n  Item Name: Vuesax Admin - VueJS Dashboard Admin Template\r\n  Author: Pixinvent\r\n  Author URL: http://www.themeforest.net/user/pixinvent\r\n========================================================================================== -->\r\n\r\n\r\n<template>\r\n  <vs-sidebar click-not-close position-right parent=\"body\" default-index=\"1\" color=\"primary\" class=\"add-new-data-sidebar items-no-padding\" spacer v-model=\"isSidebarActiveLocal\">\r\n    <div class=\"mt-6 flex items-center justify-between px-6\">\r\n        <h4>ADD NEW DATA</h4>\r\n        <feather-icon icon=\"XIcon\" @click.stop=\"isSidebarActiveLocal = false\" class=\"cursor-pointer\"></feather-icon>\r\n    </div>\r\n    <vs-divider class=\"mb-0\"></vs-divider>\r\n\r\n    <VuePerfectScrollbar class=\"scroll-area--data-list-add-new pt-4 pb-6\" :settings=\"settings\">\r\n\r\n      <div class=\"p-6\">\r\n        <!-- NAME -->\r\n        <vs-input label=\"Name\" name=\"name\" v-model=\"name\" class=\"mt-5 w-full\" />\r\n\r\n        <!-- CATEGORY -->\r\n        <vs-select v-model=\"category\" label=\"Category\" class=\"mt-5 w-full\">\r\n          <vs-select-item :key=\"item.value\" :value=\"item.value\" :text=\"item.text\" v-for=\"item in category_choices\" />\r\n        </vs-select>\r\n\r\n        <!-- ORDER STATUS -->\r\n        <vs-select v-model=\"order_status\" label=\"Order Status\" class=\"mt-5 w-full\">\r\n          <vs-select-item :key=\"item.value\" :value=\"item.value\" :text=\"item.text\" v-for=\"item in order_status_choices\" />\r\n        </vs-select>\r\n\r\n        <!-- PRICE -->\r\n        <vs-input label=\"Price\" name=\"price\" v-model=\"price\" class=\"mt-5 w-full\" />\r\n\r\n        <!-- IMG -->\r\n        <vs-upload text=\"Upload Image\" class=\"img-upload\" ref=\"fileUpload\" />\r\n      </div>\r\n    </VuePerfectScrollbar>\r\n\r\n    <div class=\"flex flex-wrap items-center justify-center p-6\" slot=\"footer\">\r\n      <vs-button class=\"mr-6\">Add Data</vs-button>\r\n      <vs-button type=\"border\" color=\"danger\" @click=\"isSidebarActiveLocal = false\">Cancel</vs-button>\r\n    </div>\r\n  </vs-sidebar>\r\n</template>\r\n\r\n<script>\r\nimport VuePerfectScrollbar from 'vue-perfect-scrollbar'\r\n\r\nexport default {\r\n  props: {\r\n    isSidebarActive: {\r\n      type: Boolean,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      name: '',\r\n      category: 'audio',\r\n      order_status: 'pending',\r\n      price: '',\r\n\r\n      category_choices: [\r\n        {text:'Audio',value:'audio'},\r\n        {text:'Computers',value:'computers'},\r\n        {text:'Fitness',value:'fitness'},\r\n        {text:'Appliance',value:'appliance'}\r\n      ],\r\n      order_status_choices: [\r\n        {text:'Pending',value:'pending'},\r\n        {text:'Canceled',value:'canceled'},\r\n        {text:'Delivered',value:'delivered'},\r\n        {text:'On Hold',value:'on_hold'}\r\n      ],\r\n      settings: { // perfectscrollbar settings\r\n          maxScrollbarLength: 60,\r\n          wheelSpeed: .60,\r\n      },\r\n    }\r\n  },\r\n  computed: {\r\n    isSidebarActiveLocal: {\r\n      get() {\r\n        return this.isSidebarActive\r\n      },\r\n      set(val) {\r\n        if(!val) {\r\n          this.$emit('closeSidebar');\r\n          this.initValues();\r\n        }\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    initValues() {\r\n      this.name = '';\r\n      this.category = 'audio';\r\n      this.order_status = 'pending';\r\n      this.price = '';\r\n      this.$refs.fileUpload.srcs = [];\r\n    }\r\n  },\r\n  components: {\r\n    VuePerfectScrollbar,\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.add-new-data-sidebar {\r\n  /deep/ .vs-sidebar--background {\r\n    z-index: 52010;\r\n  }\r\n\r\n  /deep/ .vs-sidebar {\r\n    z-index: 52010;\r\n    width: 400px;\r\n    max-width: 90vw;\r\n\r\n    .img-upload {\r\n      margin-top: 2rem;\r\n\r\n      .con-img-upload {\r\n        padding: 0;\r\n      }\r\n\r\n      .con-input-upload {\r\n        width: 100%;\r\n        margin: 0;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.scroll-area--data-list-add-new {\r\n    height: calc(100% - 4.3rem);\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/ui-elements/data-list"}]}