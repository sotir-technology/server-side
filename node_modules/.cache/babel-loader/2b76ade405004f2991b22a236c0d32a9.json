) {\n      this.disabledFrom = false;\n      this.addNewEventDialog(date);\n    },\n    openEditEvent: function openEditEvent(event) {\n      var e = this.$store.getters['calendar/eventById'](event.id);\n      this.id = e.id;\n      this.title = e.title;\n      this.start = e.start;\n      this.end = e.end;\n      this.desc = e.desc;\n      this.activePromptEditEvent = true;\n    },\n    editEvent: function editEvent() {\n      var obj = {\n        id: this.id,\n        title: this.title,\n        start: this.start,\n        end: this.end,\n        label: this.labelLocal,\n        desc: this.desc\n      };\n      obj.cssClass = \"event-\" + this.labelColor(this.labelLocal);\n      this.$store.dispatch('calendar/ediCalendarEvent', obj);\n    },\n    removeEvent: function removeEvent() {\n      this.$store.dispatch('calendar/removeCalendarEvent', this.id);\n    }\n  },\n  components: {\n    'full-calendar': require('vue-fullcalendar'),\n    Datepicker: Datepicker,\n    VuePerfectScrollbar: VuePerfectScrollbar\n  }\n};",{"version":3,"sources":["Calendar.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyIA,OAAA,UAAA,MAAA,kBAAA;AACA,OAAA,mBAAA,MAAA,uBAAA;AAEA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,KAAA,EAAA,EADA;AAEA,MAAA,KAAA,EAAA,EAFA;AAGA,MAAA,GAAA,EAAA,EAHA;AAIA,MAAA,IAAA,EAAA,EAJA;AAKA,MAAA,EAAA,EAAA,CALA;AAMA,MAAA,YAAA,EAAA,KANA;AAOA,MAAA,UAAA,EAAA,MAPA;AAQA,MAAA,QAAA,EAAA;AACA,QAAA,kBAAA,EAAA,EADA;AAEA,QAAA,UAAA,EAAA;AAFA,OARA;AAcA,MAAA,oBAAA,EAAA,KAdA;AAeA,MAAA,qBAAA,EAAA;AAfA,KAAA;AAiBA,GAnBA;AAoBA,EAAA,QAAA,EAAA;AACA,IAAA,cADA,4BACA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,QAAA,CAAA,cAAA;AACA,KAHA;AAIA,IAAA,SAJA,uBAIA;AACA,aAAA,KAAA,KAAA,IAAA,EAAA,IAAA,KAAA,KAAA,IAAA,EAAA,IAAA,KAAA,GAAA,IAAA,EAAA,IAAA,IAAA,CAAA,KAAA,CAAA,KAAA,GAAA,IAAA,IAAA,CAAA,KAAA,CAAA,KAAA,KAAA,CAAA,IAAA,CAAA;AACA,KANA;AAOA,IAAA,eAPA,6BAOA;AACA,aAAA;AAAA,QAAA,EAAA,EAAA,IAAA