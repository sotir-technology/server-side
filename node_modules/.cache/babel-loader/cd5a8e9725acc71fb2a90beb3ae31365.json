================================================================= -->\r\n\r\n\r\n<template>\r\n\t<vx-card title=\"Pie Chart\" class=\"mb-base\" code-toggler>\r\n\r\n\t\t<div class=\"mt-5\">\r\n\t\t\t<e-charts :options=\"pie\" ref=\"pie\" auto-resize />\r\n\t\t</div>\r\n\r\n\t\t<template slot=\"codeContainer\">\r\n&lt;template&gt;\r\n  &lt;e-charts :options=&quot;pie&quot; ref=&quot;pie&quot; auto-resize /&gt;\r\n&lt;/template&gt;\r\n\r\n&lt;script&gt;\r\nimport ECharts from 'vue-echarts/components/ECharts'\r\nimport 'echarts/lib/component/tooltip'\r\nimport 'echarts/lib/component/legend'\r\nimport 'echarts/lib/chart/pie'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      pie:{\r\n        tooltip: {\r\n          trigger: 'item',\r\n          formatter: '{a} &lt;br/&gt;{b} : {c} ({d}%)'\r\n        },\r\n        legend: {\r\n          orient: 'vertical',\r\n          left: 'left',\r\n          data: ['Direct interview', 'Email marketing', 'Alliance advertising', 'Video ad', 'Search engine']\r\n        },\r\n        series: [\r\n          {\r\n            name: 'Access source',\r\n            type: 'pie',\r\n            radius: '55%',\r\n            center: ['50%', '60%'],\r\n            color: ['#FF9F43','#28C76F','#EA5455','#87ceeb','#7367F0'],\r\n            data: [\r\n              {value: 335, name: 'Direct interview'},\r\n              {value: 310, name: 'Email marketing'},\r\n              {value: 234, name: 'Alliance advertising'},\r\n              {value: 135, name: 'Video ad'},\r\n              {value: 1548, name: 'Search engine'}\r\n            ],\r\n            itemStyle: {\r\n              emphasis: {\r\n                shadowBlur: 10,\r\n                shadowOffsetX: 0,\r\n                shadowColor: 'rgba(0, 0, 0, 0.5)'\r\n              }\r\n            }\r\n          }\r\n        ]       \r\n      },\r\n    }\r\n  },\r\n  components: {\r\n    ECharts\r\n  },\r\n  mounted () {\r\n    let dataIndex = -1\r\n    let pie = this.$refs.pie\r\n    let dataLen = pie.options.series[0].data.length\r\n    setInterval(() =&gt; {\r\n      pie.dispatchAction({\r\n        type: 'downplay',\r\n        seriesIndex: 0,\r\n        dataIndex\r\n      })\r\n      dataIndex = (dataIndex + 1) % dataLen\r\n      pie.dispatchAction({\r\n        type: 'highlight',\r\n        seriesIndex: 0,\r\n        dataIndex\r\n      })\r\n      pie.dispatchAction({\r\n        type: 'showTip',\r\n        seriesIndex: 0,\r\n        dataIndex\r\n      })\r\n    }, 1000)\r\n  }\r\n}\r\n&lt;/script&gt;\r\n\t\t</template>\r\n\t</vx-card>\r\n</template>\r\n\r\n<script>\r\nimport ECharts from 'vue-echarts/components/ECharts'\r\nimport 'echarts/lib/component/tooltip'\r\nimport 'echarts/lib/component/legend'\r\nimport 'echarts/lib/chart/pie'\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            pie: {\r\n                tooltip: {\r\n                    trigger: 'item',\r\n                    formatter: '{a} <br/>{b} : {c} ({d}%)'\r\n                },\r\n                legend: {\r\n                    orient: 'vertical',\r\n                    left: 'left',\r\n                    data: ['Direct interview', 'Email marketing', 'Alliance advertising', 'Video ad', 'Search engine']\r\n                },\r\n                series: [{\r\n                    name: 'Access source',\r\n                    type: 'pie',\r\n                    radius: '55%',\r\n                    center: ['50%', '60%'],\r\n                    color: ['#FF9F43', '#28C76F', '#EA5455', '#87ceeb', '#7367F0'],\r\n                    data: [\r\n                        { value: 335, name: 'Direct interview' },\r\n                        { value: 310, name: 'Email marketing' },\r\n                        { value: 234, name: 'Alliance advertising' },\r\n                        { value: 135, name: 'Video ad' },\r\n                        { value: 1548, name: 'Search engine' }\r\n                    ],\r\n                    itemStyle: {\r\n                        emphasis: {\r\n                            shadowBlur: 10,\r\n                            shadowOffsetX: 0,\r\n                            shadowColor: 'rgba(0, 0, 0, 0.5)'\r\n                        }\r\n                    }\r\n                }]\r\n            },\r\n        }\r\n    },\r\n    components: {\r\n        ECharts\r\n    },\r\n    mounted() {\r\n        let dataIndex = -1\r\n        let pie = this.$refs.pie\r\n        let dataLen = pie.options.series[0].data.length\r\n        setInterval(() => {\r\n            pie.dispatchAction({\r\n                type: 'downplay',\r\n                seriesIndex: 0,\r\n                dataIndex\r\n            })\r\n            dataIndex = (dataIndex + 1) % dataLen\r\n            pie.dispatchAction({\r\n                type: 'highlight',\r\n                seriesIndex: 0,\r\n                dataIndex\r\n            })\r\n            pie.dispatchAction({\r\n                type: 'showTip',\r\n                seriesIndex: 0,\r\n                dataIndex\r\n            })\r\n        }, 1000)\r\n    }\r\n}\r\n</script>"],"sourceRoot":"src/views/charts-and-maps/charts/echarts"}]}