 this.navbarColor;\n      },\n      set: function set(val) {\n        this.$emit('updateNavbarColor', val);\n      }\n    },\n    footerTypeLocal: {\n      get: function get() {\n        return this.footerType;\n      },\n      set: function set(val) {\n        this.$emit('updateFooter', val);\n      }\n    },\n    routerTransitionLocal: {\n      get: function get() {\n        return this.routerTransition;\n      },\n      set: function set(val) {\n        this.$emit('updateRouterTransition', val);\n      }\n    },\n    primaryColor: {\n      get: function get() {\n        return this.$store.state.themePrimaryColor;\n      },\n      set: function set(val) {\n        this.$store.commit('UPDATE_PRIMARY_COLOR', val);\n      }\n    },\n    hideScrollToTopLocal: {\n      get: function get() {\n        return this.hideScrollToTop;\n      },\n      set: function set(val) {\n        this.$emit('toggleHideScrollToTop', val);\n      }\n    }\n  },\n  methods: {\n    updatePrimaryColor: function updatePrimaryColor(color) {\n      this.primaryColor = color;\n      this.$vs.theme({\n        primary: color\n      });\n    }\n  },\n  components: {\n    VuePerfectScrollbar: VuePerfectScrollbar\n  }\n};",{"version":3,"sources":["TheCustomizer.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkHA,OAAA,mBAAA,MAAA,uBAAA;AAEA,eAAA;AACA,EAAA,KAAA,EAAA;AACA,IAAA,UAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,QAAA,EAAA;AAFA,KADA;AAKA,IAAA,WAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,QAAA,EAAA,IAFA;AAGA,MAAA,OAAA,EAAA;AAHA,KALA;AAUA,IAAA,UAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,QAAA,EAAA;AAFA,KAVA;AAcA,IAAA,gBAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,QAAA,EAAA;AAFA,KAdA;AAkBA,IAAA,eAAA,EAAA;AACA,MAAA,IAAA,EAAA,OADA;AAEA,MAAA,QAAA,EAAA;AAFA;AAlBA,GADA;AAwBA,EAAA,IAxBA,kBAwBA;AACA,WAAA;AACA,MAAA,MAAA,EAAA,KADA;AAEA,MAAA,WAAA,EAAA,CAAA,SAAA,EAAA,SAAA,EAAA,SAAA,EAAA,SAAA,EAAA,SAAA,CAFA;AAGA,MAAA,kB