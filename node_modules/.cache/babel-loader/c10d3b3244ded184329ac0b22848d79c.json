\r\n        if(this.type == 'area') {\r\n            this.chartData.chartOptions['theme'] = {\r\n                monochrome: {\r\n                    enabled: true,\r\n                    color: this.getHex(this.color),\r\n                    shadeTo: 'light',\r\n                    shadeIntensity: 0.65\r\n                }\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>"],"sourceRoot":"src/components/statistics-cards"}]}