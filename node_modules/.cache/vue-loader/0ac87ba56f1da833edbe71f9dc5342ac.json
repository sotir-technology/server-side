\n        {text:'Appliance',value:'appliance'}\n      ],\n      order_status_choices: [\n        {text:'Pending',value:'pending'},\n        {text:'Canceled',value:'canceled'},\n        {text:'Delivered',value:'delivered'},\n        {text:'On Hold',value:'on_hold'}\n      ],\n      settings: { // perfectscrollbar settings\n          maxScrollbarLength: 60,\n          wheelSpeed: .60,\n      },\n    }\n  },\n  computed: {\n    isSidebarActiveLocal: {\n      get() {\n        return this.isSidebarActive\n      },\n      set(val) {\n        if(!val) {\n          this.$emit('closeSidebar');\n          this.initValues();\n        }\n      }\n    }\n  },\n  methods: {\n    initValues() {\n      this.name = '';\n      this.category = 'audio';\n      this.order_status = 'pending';\n      this.price = '';\n      this.$refs.fileUpload.srcs = [];\n    }\n  },\n  components: {\n    VuePerfectScrollbar,\n  }\n}\n",{"version":3,"sources":["AddNewDataSidebar.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiDA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"AddNewDataSidebar.vue","sourceRoot":"src/views/churcha2z/ui-elements/data-list","sourcesContent":["<!-- =========================================================================================\n  File Name: AddNewDataSidebar.vue\n  Description: Add New Data - Sidebar component\n  ----------------------------------------------------------------------------------------\n  Item Name: Vuesax Admin - VueJS Dashboard Admin Template\n  Author: Pixinvent\n  Author URL: http://www.themeforest.net/user/pixinvent\n========================================================================================== -->\n\n\n<template>\n  <vs-sidebar click-not-close position-right parent=\"body\" default-index=\"1\" color=\"primary\" class=\"add-new-data-sidebar items-no-padding\" spacer v-model=\"isSidebarActiveLocal\">\n    <div class=\"mt-6 flex items-center justify-between px-6\">\n        <h4>{{ title }}</h4>\n        <feather-icon icon=\"XIcon\" @click.stop=\"isSidebarActiveLocal = false\" class=\"cursor-pointer\"></feather-icon>\n    </div>\n    <vs-divider class=\"mb-0\"></vs-divider>\n\n    <VuePerfectScrollbar class=\"scroll-area--data-list-add-new pt-4 pb-6\" :settings=\"settings\">\n\n      <div class=\"p-6\">\n        <!-- NAME -->\n        <vs-input v-for=\"(input,index) in inputInfo\" :key=\"index\" :label=\"input.name\" :type=\"input.type\" :name=\"input.name\" :placeholder=\"input.placeholder\" v-model=\"name\" class=\"mt-5 w-full\" />\n\n<!--        &lt;!&ndash; CATEGORY &ndash;&gt;-->\n<!--        <vs-select v-model=\"category\" label=\"Category\" class=\"mt-5 w-full\">-->\n<!--          <vs-select-item :key=\"item.value\" :value=\"item.value\" :text=\"item.text\" v-for=\"item in category_choices\" />-->\n<!--        </vs-select>-->\n\n<!--        &lt;!&ndash; ORDER STATUS &ndash;&gt;-->\n<!--        <vs-select v-model=\"order_status\" label=\"Order Status\" class=\"mt-5 w-full\">-->\n<!--          <vs-select-item :key=\"item.value\" :value=\"item.value\" :text=\"item.text\" v-for=\"item in order_status_choices\" />-->\n<!--        </vs-select>-->\n\n\n\n        <!-- IMG -->\n        <vs-upload text=\"Upload Image\" class=\"img-upload\" ref=\"fileUpload\" />\n      </div>\n    </VuePerfectScrollbar>\n\n    <div class=\"flex flex-wrap items-center justify-center p-6\" slot=\"footer\">\n      <vs-button class=\"mr-6\">Add Data</vs-button>\n      <vs-button type=\"border\" color=\"danger\" @click=\"isSidebarActiveLocal = false\">Cancel</vs-button>\n    </div>\n  </vs-sidebar>\n</template>\n\n<script>\nimport VuePerfectScrollbar from 'vue-perfect-scrollbar'\n\nexport default {\n  props: {\n\n    title: String,\n    inputInfo: Array,\n    isSidebarActive: {\n      type: Boolean,\n      required: true\n    }\n  },\n  data() {\n    return {\n      name: '',\n      category: 'audio',\n      order_status: 'pending',\n      price: '',\n\n      category_choices: [\n        {text:'Audio',value:'audio'},\n        {text:'Computers',value:'computers'},\n        {text:'Fitness',value:'fitness'},\n        {text:'Appliance',value:'appliance'}\n      ],\n      order_status_choices: [\n        {text:'Pending',value:'pending'},\n        {text:'Canceled',value:'canceled'},\n        {text:'Delivered',value:'delivered'},\n        {text:'On Hold',value:'on_hold'}\n      ],\n      settings: { // perfectscrollbar settings\n          maxScrollbarLength: 60,\n          wheelSpeed: .60,\n      },\n    }\n  },\n  computed: {\n    isSidebarActiveLocal: {\n      get() {\n        return this.isSidebarActive\n      },\n      set(val) {\n        if(!val) {\n          this.$emit('closeSidebar');\n          this.initValues();\n        }\n      }\n    }\n  },\n  methods: {\n    initValues() {\n      this.name = '';\n      this.category = 'audio';\n      this.order_status = 'pending';\n      this.price = '';\n      this.$refs.fileUpload.srcs = [];\n    }\n  },\n  components: {\n    VuePerfectScrollbar,\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.add-new-data-sidebar {\n  /deep/ .vs-sidebar--background {\n    z-index: 52010;\n  }\n\n  /deep/ .vs-sidebar {\n    z-index: 52010;\n    width: 400px;\n    max-width: 90vw;\n\n    .img-upload {\n      margin-top: 2rem;\n\n      .con-img-upload {\n        padding: 0;\n      }\n\n      .con-input-upload {\n        width: 100%;\n        margin: 0;\n      }\n    }\n  }\n}\n\n.scroll-area--data-list-add-new {\n    height: calc(100% - 4.3rem);\n}\n</style>\n"]}]}