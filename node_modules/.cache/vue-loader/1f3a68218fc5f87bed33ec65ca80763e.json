--------------\r\n    Item Name: Vuesax Admin - VueJS Dashboard Admin Template\r\n      Author: Pixinvent\r\n    Author URL: http://www.themeforest.net/user/pixinvent\r\n========================================================================================== -->\r\n\r\n<template>\r\n    <v-tour name=\"vuesaxTour\" :steps=\"steps\">\r\n        <template slot-scope=\"tour\">\r\n            <transition name=\"fade\">\r\n                <v-step\r\n                    v-if=\"tour.currentStep === index\"\r\n                    v-for=\"(step, index) of tour.steps\"\r\n                    :key=\"index\"\r\n                    :step=\"step\"\r\n                    :previous-step=\"tour.previousStep\"\r\n                    :next-step=\"tour.nextStep\"\r\n                    :stop=\"tour.stop\"\r\n                    :is-first=\"tour.isFirst\"\r\n                    :is-last=\"tour.isLast\"\r\n                    :labels=\"tour.labels\">\r\n\r\n                    <div slot=\"actions\" class=\"flex justify-center\">\r\n                        <vs-button\r\n                            size=\"small\"\r\n                            class=\"mr-3\"\r\n                            @click=\"tour.stop\"\r\n                            icon-pack=\"feather\"\r\n                            icon=\"icon-x\"\r\n                            icon-after\r\n                            color=\"#fff\"\r\n                            type=\"border\"\r\n                            v-if=\"tour.currentStep != tour.steps.length - 1\">\r\n                            Skip\r\n                        </vs-button>\r\n\r\n                        <vs-button\r\n                            size=\"small\"\r\n                            @click=\"tour.previousStep\"\r\n                            icon-pack=\"feather\"\r\n                            icon=\"icon-chevrons-left\"\r\n                            color=\"#fff\"\r\n                            type=\"border\"\r\n                            class=\"mr-3\"\r\n                            v-if=\"tour.currentStep\">\r\n                            Previous\r\n                        </vs-button>\r\n\r\n                        <vs-button\r\n                            size=\"small\"\r\n                            @click=\"tour.nextStep\"\r\n                            icon-pack=\"feather\"\r\n                            icon=\"icon-chevrons-right\"\r\n                            icon-after\r\n                            color=\"#fff\"\r\n                            type=\"border\"\r\n                            class=\"btn-tour-next\"\r\n                            v-if=\"tour.currentStep != tour.steps.length - 1\">\r\n                            Next\r\n                        </vs-button>\r\n\r\n                        <vs-button\r\n                            size=\"small\"\r\n                            @click=\"tour.stop\"\r\n                            icon-pack=\"feather\"\r\n                            icon=\"icon-check-circle\"\r\n                            icon-after\r\n                            color=\"#fff\"\r\n                            type=\"border\"\r\n                            class=\"btn-tour-finish\"\r\n                            v-if=\"tour.currentStep == tour.steps.length - 1\">\r\n                            Finish\r\n                        </vs-button>\r\n                    </div>\r\n\r\n                </v-step>\r\n            </transition>\r\n        </template>\r\n    </v-tour>\r\n</template>\r\n\r\n<script>\r\nexport default{\r\n    name: 'vx-tour',\r\n    props: {\r\n        steps: {\r\n            required: true,\r\n            type: Array\r\n        }\r\n    },\r\n    mounted() {\r\n        this.$tours['vuesaxTour'].start()\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.v-tour {\r\n    .v-step {\r\n        z-index: 55000;\r\n        background-color: rgba(var(--vs-primary),1);\r\n        border-radius: .5rem;\r\n        padding: 1.5rem;\r\n        filter: drop-shadow(0 0 7px rgba(0,0,0,.5));\r\n\r\n        .v-step__arrow {\r\n            border-color: rgba(var(--vs-primary),1);\r\n        }\r\n\r\n        .vs-button-border:not(.btn-tour-next):not(.btn-tour-finish) {\r\n            border-color: rgba(255, 255, 255, .5) !important;\r\n        }\r\n    }\r\n}\r\n</style>"]}]}