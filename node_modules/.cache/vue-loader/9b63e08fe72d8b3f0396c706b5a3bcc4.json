\r\n\r\n<script>\r\nexport default {\r\n    data: () => ({\r\n        badge1: 2,\r\n        badge2: 10,\r\n    }),\r\n    methods: {\r\n        increment() {\r\n            this.badge1++\r\n            this.badge2++\r\n        }\r\n    }\r\n}\r\n</script>"]}]}