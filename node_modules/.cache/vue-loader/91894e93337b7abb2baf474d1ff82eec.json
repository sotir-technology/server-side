quot;primary&quot; type=&quot;border&quot;&gt;Run prompt inputs&lt;/vs-button&gt;\r\n      &lt;div class=&quot;op-block&quot;&gt;Security Code: {{ \"\\{\\{ val == null ? 'null' : val \\}\\}\" }} &lt;/div&gt;\r\n      &lt;div class=&quot;op-block&quot;&gt;\r\n        Name: {{ \"\\{\\{ valMultipe.value1 \\}\\}\" }} | Last Name: {{ \"\\{\\{ valMultipe.value2 \\}\\}\" }}\r\n      &lt;/div&gt;\r\n\r\n    &lt;/div&gt;\r\n\r\n    &lt;vs-prompt\r\n      @vs-cancel=&quot;val=''&quot;\r\n      @vs-accept=&quot;acceptAlert&quot;\r\n      @vs-close=&quot;close&quot;\r\n      :vs-active.sync=&quot;activePrompt&quot;&gt;\r\n      &lt;div class=&quot;con-exemple-prompt&quot;&gt;\r\n        &lt;span&gt;Enter the security code&lt;/span&gt;\r\n      &lt;vs-input placeholder=&quot;Code&quot; vs-placeholder=&quot;Code&quot; v-model=&quot;val&quot; class=&quot;mt-3 w-full&quot; /&gt;\r\n      &lt;/div&gt;\r\n    &lt;/vs-prompt&gt;\r\n\r\n    &lt;vs-prompt\r\n      @vs-cancel=&quot;clearValMultiple&quot;\r\n      @vs-accept=&quot;acceptAlert&quot;\r\n      @vs-close=&quot;close&quot;\r\n      :vs-is-valid=&quot;validName&quot;\r\n      :vs-active.sync=&quot;activePrompt2&quot;&gt;\r\n      &lt;div class=&quot;con-exemple-prompt&quot;&gt;\r\n        Enter your first and last name to &lt;b&gt;continue&lt;/b&gt;.\r\n      &lt;vs-input placeholder=&quot;Name&quot; v-model=&quot;valMultipe.value1&quot; class=&quot;mt-4 mb-2 w-full&quot; /&gt;\r\n      &lt;vs-input placeholder=&quot;Last Name&quot; v-model=&quot;valMultipe.value2&quot; class=&quot;w-full&quot; /&gt;\r\n\r\n      &lt;vs-alert :vs-active=&quot;!validName&quot; color=&quot;danger&quot; vs-icon=&quot;new_releases&quot; &gt;\r\n        Fields can not be empty please enter the data\r\n      &lt;/vs-alert&gt;\r\n      &lt;/div&gt;\r\n    &lt;/vs-prompt&gt;\r\n&lt;/template&gt;\r\n\r\n&lt;script&gt;\r\nexport default {\r\n  data(){\r\n    return {\r\n      activePrompt:false,\r\n      activePrompt2:false,\r\n      val:'',\r\n      valMultipe:{\r\n        value1:'',\r\n        value2:''\r\n      },\r\n    }\r\n  },\r\n  computed:{\r\n    validName(){\r\n      return (this.valMultipe.value1.length &gt; 0 &amp;&amp; this.valMultipe.value2.length &gt; 0)\r\n    }\r\n  },\r\n  methods:{\r\n    acceptAlert(){\r\n      this.clearValMultiple();\r\n      this.$vs.notify({\r\n        color:'success',\r\n        title:'Accept Selected',\r\n        text:'Lorem ipsum dolor sit amet, consectetur'\r\n      })\r\n    },\r\n    close(){\r\n      this.$vs.notify({\r\n        color:'danger',\r\n        title:'Closed',\r\n        text:'You close a dialog!'\r\n      })\r\n    },\r\n    clearValMultiple() {\r\n      this.valMultipe.value1 = &quot;&quot;;\r\n      this.valMultipe.value2 = &quot;&quot;;\r\n    }\r\n  }\r\n}\r\n&lt;/script&gt;\r\n\t\t</template>\r\n\r\n\t</vx-card>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data(){\r\n    return {\r\n      activePrompt:false,\r\n      activePrompt2:false,\r\n      val:'',\r\n      valMultipe:{\r\n        value1:'',\r\n        value2:''\r\n      },\r\n    }\r\n  },\r\n  computed:{\r\n    validName(){\r\n      return (this.valMultipe.value1.length > 0 && this.valMultipe.value2.length > 0)\r\n    }\r\n  },\r\n  methods:{\r\n    acceptAlert(){\r\n      this.clearValMultiple();\r\n      this.$vs.notify({\r\n        color:'success',\r\n        title:'Accept Selected',\r\n        text:'Lorem ipsum dolor sit amet, consectetur'\r\n      })\r\n    },\r\n    close(){\r\n      this.$vs.notify({\r\n        color:'danger',\r\n        title:'Closed',\r\n        text:'You close a dialog!'\r\n      })\r\n    },\r\n    clearValMultiple() {\r\n      this.valMultipe.value1 = \"\";\r\n      this.valMultipe.value2 = \"\";\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]}]}