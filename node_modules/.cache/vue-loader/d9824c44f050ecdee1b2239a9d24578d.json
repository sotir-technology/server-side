\n\n\n\n@import \"@/assets/scss/vuesax/pages/profile.scss\";\n",{"version":3,"sources":["profileSettings.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsNA","file":"profileSettings.vue","sourceRoot":"src/views/profile","sourcesContent":["<template>\n    <div id=\"profile-settings\">\n         <!-- COL AREA -->\n        <div class=\"vx-row\">\n            <!-- COL 1 -->\n            <div class=\"vx-col w-full lg:w-1/4\">\n                <!-- ABOUT CARD -->\n                <vx-card title=\"About\" class=\"mt-base\">\n                    <!-- ACTION SLOT -->\n                    <template slot=\"actions\">\n                        <feather-icon icon=\"MoreHorizontalIcon\"></feather-icon>\n                    </template>\n\n                    <!-- USER BIO -->\n                    <div class=\"user-bio\">\n                        <p>Tart I love sugar plum I love oat cake. Sweet roll caramels I love jujubes. Topping cake wafer.</p>\n                    </div>\n\n                    <!-- OTEHR DATA -->\n                    <div class=\"mt-5\">\n                        <h6>Joined:</h6>\n                        <p>November 15, 2015</p>\n                    </div>\n\n                    <div class=\"mt-5\">\n                        <h6>Lives:</h6>\n                        <p>New York, USA</p>\n                    </div>\n\n                    <div class=\"mt-5\">\n                        <h6>Email:</h6>\n                        <p>bucketful@fiendhead.org</p>\n                    </div>\n\n                    <div class=\"mt-5\">\n                        <h6>Website:</h6>\n                        <p>www.pixinvent.com</p>\n                    </div>\n\n                    <div class=\"social-links flex mt-4\">\n                        <feather-icon svgClasses=\"h-7 w-7 cursor-pointer bg-primary p-1 text-white rounded\" class=\"mr-2\" icon=\"FacebookIcon\"></feather-icon>\n                        <feather-icon svgClasses=\"h-7 w-7 cursor-pointer bg-primary p-1 text-white rounded\" class=\"mr-2\" icon=\"TwitterIcon\"></feather-icon>\n                        <feather-icon svgClasses=\"h-7 w-7 cursor-pointer bg-primary p-1 text-white rounded\" class=\"mr-2\" icon=\"InstagramIcon\"></feather-icon>\n                    </div>\n                </vx-card>\n            </div>\n\n            <div class=\"vx-col w-full lg:w-3/4\">\n                <vx-card title=\"Account Settings\" class=\"mt-base\">\n                    <div class=\"\">\n                        <form class=\"\" @submit.prevent=\"submitFormData\">\n                            <div class=\"vx-row mb-2\">\n                                <div class=\"vx-col w-full lg:w-1/2\">\n                                </div>\n                            </div>\n                            <div class=\"vx-row mb-2\">\n                                <div class=\"vx-col w-full\">\n                                    <vs-input \n                                        class=\"w-full\" \n                                        icon-pack=\"feather\" \n                                        icon=\"icon-user\" \n                                        icon-no-border \n                                        label-placeholder=\"Full Name\" \n                                        name=\"accountName\" \n                                        v-model=\"profile.name\"\n                                        @input=\"profile($event);\"\n                                         />\n                                </div>\n                            </div>\n                            <div class=\"vx-row mb-2\">\n                                <div class=\"vx-col w-full\">\n                                <vs-input type=\"email\" \n                                    class=\"w-full\" \n                                    icon-pack=\"feather\" \n                                    icon=\"icon-mail\" \n                                    icon-no-border \n                                    label-placeholder=\"Email\"\n                                     name=\"email\"  \n                                     v-model=\"profile.email\"/>\n                                </div>\n                            </div>\n                            <div class=\"vx-row mb-2\">\n                                <div class=\"vx-col w-full\">\n                                    <vs-input \n                                        type=\"tel\" \n                                        class=\"w-full\" \n                                        icon-pack=\"feather\" \n                                        icon=\"icon-smartphone\" \n                                        icon-no-border\n                                        label-placeholder=\"Mobile\" \n                                        name=\"phone\" \n                                        v-model=\"profile.phone\" />\n                                </div>\n                            </div>\n                            <div class=\"vx-row mb-2\">\n                                <div class=\"vx-col w-full\">\n                                    <vs-input \n                                        type=\"tel\" \n                                        class=\"w-full\" \n                                        icon-pack=\"feather\" \n                                        icon=\"icon-globe\" \n                                        icon-no-border\n                                        label-placeholder=\"Country\" \n                                        name=\"country\" \n                                        v-model=\"profile.country\" />\n                                </div>\n                            </div>\n                            <div class=\"vx-row mb-2\">\n                                <div class=\"vx-col w-full\">\n                                    <vs-input \n                                        type=\"tel\" \n                                        class=\"w-full\" \n                                        icon-pack=\"feather\" \n                                        icon=\"icon-globe\" \n                                        icon-no-border\n                                        label-placeholder=\"State\" \n                                        name=\"country\" \n                                        v-model=\"profile.state\" />\n                                </div>\n                            </div>\n                            <div class=\"vx-row mb-2\">\n                                <div class=\"vx-col w-full\">\n                                    <vs-input \n                                        type=\"tel\" \n                                        class=\"w-full\" \n                                        icon-pack=\"feather\" \n                                        icon=\"icon-map\"\n                                        icon-no-border \n                                        label-placeholder=\"Address\" \n                                        name=\"address\" \n                                        v-model=\"profile.street\" />\n                                </div>\n                            </div>\n                            <div class=\"vx-row\">\n                                <div class=\"vx-col w-full\">\n                                    <vs-button class=\"mr-3 mb-2 mt-2\">Submit</vs-button>\n                                </div>\n                            </div>\n                        </form>\n                    </div>\n                </vx-card>\n            </div>\n\n        </div>\n    </div>\n</template>\n\n<script>\nimport { mapState } from 'vuex';\n\nexport default {\n    data() {\n        return {\n            country:3,\n            countries:[\n                {text:'Nigeria',value:0},\n                {text:'Kenya',value:1},\n                {text:'Ghana',value:2},\n                {text:'South Africa',value:3},\n            ],\n            state:3,\n            states:[\n                {text:'Abia',value:0},\n                {text:'Adamawa',value:1},\n                {text:'Akwa-ibom',value:2},\n                {text:'Anambra',value:3},\n            ],\n            // password:'',\n            // rePassword:'',\n        }\n    },\n    methods: {\n        handleWindowResize(event) {\n            this.windowWidth = event.currentTarget.innerWidth;\n            this.setSidebarWidth();\n        },\n        setSidebarWidth() {\n            if(this.windowWidth < 992) {\n                this.isFilterSidebarActive = this.clickNotClose = false;\n            }else {\n                this.isFilterSidebarActive = this.clickNotClose = true;\n            }\n        },\n        submitFormData: function(e){\n            var formDataName = this.accountName;\n            //console.log(formDataName);\n        }\n    },\n    computed: {\n        //...mapState(['userProfile']),\n        profile: {\n            get () {\n                return this.$store.state.userProfile\n            },\n            set (value) {\n                this.$store.commit('UPDATE_PROFILE', value)\n            }\n        }\n        // profileDetails(){\n        //     return this.$store.state.userProfile;\n        // }\n    },\n    components: {\n    },\n    mounted() {\n        this.wasSidebarOpen = this.$store.state.reduceButton;\n        this.$store.commit('TOGGLE_REDUCE_BUTTON', true)\n    },\n    beforeDestroy() {\n        if (!this.wasSidebarOpen) this.$store.commit('TOGGLE_REDUCE_BUTTON', false)\n    }\n}\n</script>\n<style lang=\"scss\">\n    @import \"@/assets/scss/vuesax/pages/profile.scss\";\n</style>"]}]}